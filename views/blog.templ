package views

templ Blog(content templ.Component, title string) {
	@Base() {
		<div class="container max-w-4xl mx-auto mt-4">
			<div class="mx-auto">
				<h2 class="text-3xl font-semibold text-center">{ title }</h2>
			</div>
			<div class="prose mx-auto mt-8">
				@content
			</div>
		</div>
	}
}
